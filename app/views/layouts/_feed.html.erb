<% if @feed_items %>
  <ol class="microposts">
    <%#= @feed_items.each do |feed_item|%>
      <%#= render feed_item %>
    <%# end %>
    <%= render @feed_items %>
    <%# @feed_itemsの各要素がMicropostクラスを持つため
    RailsはMicropostパーシャルを探すことができる %>  
    </ol>
    <%= will_paginate @feed_items, 
                      params: { controller: :static_pages, action: :home } %>
<% end %>